{
    "name": "switchcat/core",
    "description": "SwitchCat framework core bundle",
    "type": "library",
    "keywords": ["switchcat", "framework", "core"],
    "homepage": "https://switchcat.agency",
    "require": {
        "symfony/yaml": "^5.1",
        "symfony/http-foundation": "^5.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "phpunit/php-code-coverage": "^9.2",
        "phpstan/phpstan-phpunit": "^0.12.16"
    },
    "autoload": {
        "psr-4": {
            "SwitchCat\\Control\\": "Control/",
            "SwitchCat\\Cli\\": "Cli/",
            "SwitchCat\\Connections\\": "Connections/",
            "SwitchCat\\Exception\\": "Exception/",
            "SwitchCat\\Data\\": "Data/",
            "SwitchCat\\Initializer\\": "Initializer/",
            "SwitchCat\\Cache\\": "Cache/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Felix Eloy",
            "email": "contact@switchcat.agency"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
